{
  "name": "electron-app",
  "version": "1.0.0",
  "description": "An Electron application with React",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "start": "electron-forge start",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "npx electron-builder install-app-deps",
    "build:unpack": "npm run build && npx electron-builder --dir",
    "build:win": "npm run build && npx electron-builder --win",
    "build:mac": "npm run build && npx electron-builder --mac",
    "build:linux": "npm run build && npx electron-builder --linux",
    "package": "electron-forge package",
    "make": "npm run build && electron-forge make",
    "make:clean": "electron-forge make"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "electron-squirrel-startup": "^1.0.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.2",
    "@electron-forge/maker-deb": "^7.8.2",
    "@electron-forge/maker-rpm": "^7.8.2",
    "@electron-forge/maker-squirrel": "^7.8.2",
    "@electron-forge/maker-zip": "^7.8.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.2",
    "@electron-forge/plugin-fuses": "^7.8.2",
    "@electron-toolkit/eslint-config": "^2.0.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron/fuses": "^1.8.0",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^37.2.3",
    "electron-vite": "^4.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "vite": "^7.0.5"
  }
}
